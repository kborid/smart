            </intent-filter>
        </activity>-->

        <!--<activity android:name="com.kborid.smart.activity.FragmentContainerActivity" />-->
        <activity android:name=".imageloader.PictureActivity" />

        <service android:name="com.kborid.smart.service.SmartCounterService" />
        <service android:name=".service.LocationService"/>

        <receiver android:name=".boradcast.TestBroadcastReceiver">
